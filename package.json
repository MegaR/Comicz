{
  "name": "comicz",
  "private": true,
  "version": "2.0.0",
  "description": "",
  "license": "MIT",
  "pre-commit": [
    "format",
    "lint"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.server.json",
    "build:frontend": "webpack --config webpack.config.ts",
    "format": "prettier --write \"backend/**/*.ts\" \"frontend/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register dist/backend/main.ts",
    "start:dev": "tsc-watch -p tsconfig.server.json --onSuccess \"node dist/backend/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.server.json --onSuccess \"node --inspect-brk dist/backend/main.js\"",
    "start:frontend": "webpack --config webpack.config.ts --watch",
    "start:prod": "node dist/backend/main.js",
    "lint": "tslint -p tsconfig.server.json -c tslint.json && tslint -p tsconfig.app.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.13",
    "@nestjs/core": "^7.6.13",
    "@nestjs/graphql": "^6.4.3",
    "@nestjs/microservices": "^7.6.13",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/websockets": "^7.6.13",
    "@types/graphql-upload": "^8.0.4",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/webpack": "^4.41.26",
    "apollo-server-express": "^2.21.0",
    "graphql": "^14.7.0",
    "graphql-tools": "^4.0.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.0",
    "rxjs": "6.5.2",
    "ts-loader": "^8.0.17",
    "type-graphql": "^0.17.6",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.8",
    "@nestjs/testing": "6.5.3",
    "@types/express": "4.17.1",
    "@types/jest": "24.0.17",
    "@types/node": "12.7.2",
    "@types/supertest": "2.0.8",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "html-webpack-plugin": "^5.2.0",
    "jest": "24.9.0",
    "pre-commit": "^1.2.2",
    "prettier": "1.18.2",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tsc-watch": "2.4.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.18.0",
    "typescript": "^3.9.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "backend",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
